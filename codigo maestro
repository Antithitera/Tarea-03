double fase (double s, double r, double &phi0){
double a, b;
if(s>=1)
  phi0=pi/2;
  s=a;
else if (s<=-1)
    phi0=-pi/2;
    s=b;
if (s!=a && s!=b)
  phi0=asin (s);
return
}
double normaliza (double theta){
double n, thetaprima;
n=(theta+pi)/2/pi;
thetaprima=theta-2*n*pi
return thetaprima;
}
